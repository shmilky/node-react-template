{
  "name": "client",
  "version": "0.0.1",
  "description": "Template for creating a new project with a node (express) server and a react client",
  "dependencies": {
    "es6-promise": "4.1.0",
    "raw-loader": "0.5.1",
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "0.28.0",
    "fast-sass-loader": "1.2.5",
    "node-sass": "4.5.2",
    "path": "0.12.7",
    "style-loader": "0.16.1",
    "extract-text-webpack-plugin": "2.1.2",
    "uglifyjs-webpack-plugin": "1.0.1",
    "webpack": "3.5.3",
    "webpack-node-externals": "1.6.0"
  },
  "peerDependencies": {
    "jquery": "3.2.1",
    "lodash": "4.17.4",
    "moment": "2.21.0",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-redux": "5.0.4",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.5",
    "redux": "3.6.0",
    "redux-form": "6.6.3",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "webpack-dev-server": "2.7.1"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --open --env=dev --hot",
    "start-server": "node ../../server/bin/www",
    "start-with-server": "concurrently --kill-others \"npm run start-server\" \"npm start\"",
    "compile": "NODE_ENV=production webpack --env=prod --config webpack.config.js --display verbose --progress",
    "compile-windows": "SET NODE_ENV=production & webpack --env=prod --config webpack.config.js --display verbose --progress",
    "compile-dev": "webpack --env=dev --config webpack.config.js",
    "compile-ssr": "NODE_ENV=production webpack --env=ssr --config webpack.config.js",
    "compile-ssr-windows": "SET NODE_ENV=production & webpack --env=ssr --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
